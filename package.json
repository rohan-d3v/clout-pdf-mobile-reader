{
  "name": "clout-pdf-mobile-reader",
  "version": "0.5.1",
  "description": "a fork of a mobile-friendly  PDF Reader in browser for React",
  "keywords": [
    "react",
    "pdf",
    "pdfjs",
    "react-read-pdf"
  ],
  "main": "lib/app.js",
  "author": "yuxl",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohan-d3v/clout-pdf-mobile-reader.git.git"
  },
  "bugs": {
    "url": "https://github.com/rohan-d3v/clout-pdf-mobile-reader.git/issues"
  },
  "homepage": "https://github.com/rohan-d3v/clout-pdf-mobile-reader.git",
  "scripts": {
    "build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rm -f -r -d dist",
    "clean-lib": "rm -f -r -d lib",
    "lint": "yarn run lint:ts && yarn run lint:sass",
    "lint:ts": "tslint --fix './src/**/*.ts*' --format stylish --force",
    "lint:sass": "stylelint ./src/**/*.scss",
    "start": "npm run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js --open",
    "start-prod": "yarn run build && node express.js",
    "test": "jest --watch --coverage --config=configs/jest.json",
    "lib": "npm run clean-lib && webpack --config=configs/webpack/lib.js  --progress --color"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.32",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.2.5",
    "css-loader": "^1.0.0",
    "express": "^4.18.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.1.0",
    "jest": "^28.1.0",
    "node-sass": "^7.0.1",
    "react": "^18.1.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^18.1.0",
    "react-hot-loader": "^4.13.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.2",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-webpack-plugin": "^3.2.0",
    "tslint": "^5.20.1",
    "typescript": "^4.6.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-middleware": "^5.3.1",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-2": "^6.24.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "pdfjs-dist": "^2.13.216",
    "react-css-modules": "^4.7.11",
    "ts-loader": "^9.3.0",
    "typings-for-css-modules-loader": "^1.7.0",
    "url-loader": "^4.1.1"
  }
}
